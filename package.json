  ],
  "main": "./dist/vue-email.umd.cjs",
  "module": "./dist/vue-email.js",
  "exports": {
    ".": {
      "import": "./dist/vue-email.js",
      "require": "./dist/vue-email.umd.cjs"
    }
  },
  "bugs": {
    "url": "https://github.com/Dave136/vue-email/issues"
  },
  "types": "./dist/index.d.ts",
  "author": {
    "name": "David Arenas"
  },
  "scripts": {
